issn = {15731375},
journal = {Statistics and Computing},
keywords = {Dependence modeling,Lasso,Sparsity,Vine copula},
number = {2},
pages = {269--287},
publisher = {Springer US},
title = {{Selection of sparse vine copulas in high dimensions with the Lasso}},
url = {https://doi.org/10.1007/s11222-018-9807-5},
volume = {29},
year = {2019}
}
@article{Gruber2018,
abstract = {Regular vine copulas are a flexible class of dependence models, but Bayesian methodology for model selection and inference is not yet fully developed. We propose sparsity-inducing but otherwise non-informative priors, and present novel proposals to enable reversible jump Markov chain Monte Carlo posterior simulation for Bayesian model selection and inference. Our method is the first to jointly estimate the posterior distribution of all trees of a regular vine copula. This represents a substantial improvement over existing frequentist and Bayesian strategies, which can only select one tree at a time and are known to induce bias. A simulation study demonstrates the feasibility of our strategy and shows that it combines superior selection and reduced computation time compared to Bayesian tree-by-tree selection. In a real data example, we forecast the daily expected tail loss of a portfolio of nine exchange-traded funds using a fully Bayesian multivariate dynamic model built around Bayesian regular vine copulas to illustrate our model's viability for financial analysis and risk estimation.},
author = {Gruber, Lutz F. and Czado, Claudia},
doi = {10.1214/17-BA1089},
issn = {19316690},
journal = {Bayesian Analysis},
keywords = {Bayesian inference,Copula modeling,Dependence modeling,Financial analysis,Importance sampling,Multivariate analysis,Posterior simulation,Risk forecasting,Simulation studies,Vine copulas},
number = {4},
