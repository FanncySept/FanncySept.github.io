file = {:C$\backslash$:/Users/Fanncy/Desktop/Vine Copula Working{\_}0305/Desktop VineCopula{\_}1107/2009{\_}Cvine{\_}Czado{\_}Pair-copula constructions of multiple{\_}Classical.pdf:pdf},
